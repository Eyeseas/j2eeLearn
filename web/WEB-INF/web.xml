<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>com.kong.useJsoup.view.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ParseBookServlet</servlet-name>
        <servlet-class>com.kong.useJsoup.view.ParseBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParseBookServlet</servlet-name>
        <url-pattern>/ParseBookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChapterContentServlet</servlet-name>
        <servlet-class>com.kong.useJsoup.view.ChapterContentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChapterContentServlet</servlet-name>
        <url-pattern>/ChapterContentServlet</url-pattern>
    </servlet-mapping>
</web-app>